<%- include('include/head.ejs') %>
    <% if(users.length === 0){ %>
        <h1>Хэрэглэг байхгүй байна..</h1>
    <% } else { %>
        <% for(user of users){ %>
            <h1 class="text-green-400">
               <a href="/user/%<= user.id %>">
                    <%= user.name %>
               </a>
            </h1>
        <% } %>
    <% } %>
<%- include('include/foot.ejs') %>
<script src="/js/index.js"></script>